{
    "client_id": "CLIENT_001",
    "client_name": "Sample Client Corporation",
    
    "data_processing": {
        "date_format": "%Y-%m-%d",
        "encoding": "utf-8",
        "decimal_places": 2,
        "input_column_mapping": {
            "employee_id": "EmployeeID",
            "claim_date": "ServiceDate",
            "paid_date": "PaidDate",
            "billed_amount": "BilledAmount",
            "allowed_amount": "AllowedAmount",
            "paid_amount": "PaidAmount"
        }
    },
    
    "pharmacy_rebates": {
        "enabled": true,
        "calculation_method": "tiered",
        "flat_percentage": 0.20,
        "tiers": {
            "Generic": 0.75,
            "Preferred Brand": 0.25,
            "Non-Preferred Brand": 0.15,
            "Specialty": 0.10
        },
        "minimum_rebate_threshold": 100.00,
        "apply_to_mail_order_only": false
    },
    
    "stop_loss": {
        "enabled": true,
        "contract_type": "specific_and_aggregate",
        "specific_attachment_point": 250000,
        "specific_corridor": 50000,
        "aggregate_attachment_factor": 1.25,
        "reimbursement_percentage": 1.0,
        "reimbursement_lag_months": 2,
        "lasering": {
            "enabled": false,
            "members": []
        }
    },
    
    "fixed_costs": {
        "admin_fee_pepm": 35.00,
        "stop_loss_premium_pepm": 65.00,
        "network_access_pmpm": 2.50,
        "wellness_program_pepm": 5.00,
        "disease_management_pepm": 8.00,
        "telemedicine_pepm": 3.00,
        "other_fees": {
            "pbm_admin_fee_pmpm": 1.50,
            "data_warehouse_fee_monthly": 500.00
        }
    },
    
    "budget": {
        "budget_year": 2024,
        "medical_pmpm": 425.00,
        "pharmacy_pmpm": 125.00,
        "total_pmpm": 550.00,
        "annual_trend": 0.08,
        "confidence_interval": 0.95,
        "budget_by_month": {
            "1": 550.00,
            "2": 550.00,
            "3": 560.00,
            "4": 560.00,
            "5": 560.00,
            "6": 570.00,
            "7": 570.00,
            "8": 570.00,
            "9": 580.00,
            "10": 580.00,
            "11": 590.00,
            "12": 600.00
        }
    },
    
    "high_cost_analysis": {
        "threshold_amount": 50000,
        "catastrophic_threshold": 100000,
        "include_dependents": true,
        "diagnosis_grouping": true,
        "anonymize_under_threshold": 10
    },
    
    "reporting": {
        "include_member_detail": false,
        "anonymize_ids": true,
        "hash_algorithm": "sha256",
        "rolling_months": 24,
        "comparison_periods": ["prior_month", "prior_quarter", "prior_year", "ytd"],
        "fiscal_year_start_month": 1,
        "report_segments": ["division", "location", "plan_type", "coverage_tier"],
        "custom_groupings": {
            "regions": {
                "West": ["CA", "OR", "WA", "NV", "AZ"],
                "Central": ["TX", "OK", "KS", "MO", "IL"],
                "East": ["NY", "NJ", "PA", "MA", "FL"]
            }
        }
    },
    
    "validation": {
        "outlier_threshold_zscore": 3.0,
        "outlier_threshold_iqr_multiplier": 1.5,
        "max_monthly_variance_warning": 0.20,
        "max_pmpm_variance_error": 0.50,
        "required_data_completeness": 0.95,
        "duplicate_claim_window_days": 30,
        "expected_claim_lag_days": 45
    },
    
    "visualization": {
        "theme": "professional",
        "color_scheme": "blue_teal",
        "chart_style": "modern_flat",
        "logo_path": "/assets/client_logo.png",
        "show_logo_on_all_pages": true,
        "brand_colors": {
            "primary": "#003B5C",
            "secondary": "#0099A8", 
            "accent": "#66C3FF",
            "positive": "#00A651",
            "negative": "#D32030",
            "warning": "#FFB81C",
            "neutral": "#6B7B8A",
            "background": "#F7F8FA",
            "text": "#2C3E50"
        },
        "font_settings": {
            "primary_font": "Roboto",
            "secondary_font": "Open Sans",
            "monospace_font": "Roboto Mono"
        },
        "dashboard_settings": {
            "show_filters_pane": true,
            "default_time_period": "last_12_months",
            "enable_drill_through": true,
            "enable_export": true,
            "refresh_interval_minutes": 1440
        }
    },
    
    "notifications": {
        "enabled": true,
        "email_recipients": ["analytics@company.com"],
        "alert_thresholds": {
            "budget_variance_percent": 10,
            "high_cost_claims_count": 5,
            "data_quality_score_minimum": 85
        },
        "report_schedule": "monthly",
        "send_validation_summary": true
    },
    
    "data_retention": {
        "raw_data_months": 36,
        "processed_data_months": 60,
        "archive_location": "archive/",
        "compression": "gzip"
    }
}