{
    "name": "Healthcare Benefits Dashboard",
    "dataColors": [
        "#1E4D8B",
        "#16A085", 
        "#3498DB",
        "#27AE60",
        "#E74C3C",
        "#F39C12",
        "#9B59B6",
        "#34495E"
    ],
    "background": "#FAFAFA",
    "foreground": "#2C3E50",
    "tableAccent": "#1E4D8B",
    
    "textClasses": {
        "title": {
            "fontFace": "Roboto",
            "fontSize": 16,
            "fontWeight": "bold",
            "color": "#2C3E50"
        },
        "label": {
            "fontFace": "Roboto",
            "fontSize": 11,
            "color": "#34495E"
        },
        "callout": {
            "fontFace": "Roboto",
            "fontSize": 28,
            "fontWeight": "bold",
            "color": "#1E4D8B"
        },
        "header": {
            "fontFace": "Roboto",
            "fontSize": 14,
            "fontWeight": "semibold",
            "color": "#2C3E50"
        }
    },
    
    "visualStyles": {
        "*": {
            "*": {
                "border": [{
                    "show": true,
                    "color": { "solid": { "color": "#E8E8E8" } },
                    "radius": 8,
                    "width": 1
                }],
                "background": [{
                    "show": true,
                    "color": { "solid": { "color": "#FFFFFF" } },
                    "transparency": 0
                }],
                "padding": [{
                    "top": 10,
                    "bottom": 10,
                    "left": 10,
                    "right": 10
                }],
                "general": [{
                    "responsive": true,
                    "keepLayerOrder": true
                }],
                "title": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#2C3E50" } },
                    "background": { "solid": { "color": "#FFFFFF" } },
                    "alignment": "center",
                    "fontSize": 14,
                    "fontFamily": "Roboto"
                }]
            }
        },
        
        "card": {
            "*": {
                "categoryLabels": [{
                    "show": true,
                    "color": { "solid": { "color": "#34495E" } },
                    "fontSize": 11,
                    "fontFamily": "Roboto"
                }],
                "wordWrap": [{
                    "show": true
                }],
                "dataLabels": [{
                    "color": { "solid": { "color": "#1E4D8B" } },
                    "fontSize": 32,
                    "fontFamily": "Roboto",
                    "fontWeight": "bold"
                }]
            }
        },
        
        "lineChart": {
            "*": {
                "legend": [{
                    "show": true,
                    "position": "Bottom",
                    "showTitle": false,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }],
                "categoryAxis": [{
                    "show": true,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "axisScale": "linear",
                    "gridlineShow": true,
                    "gridlineColor": { "solid": { "color": "#E8E8E8" } },
                    "gridlineThickness": 1,
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }],
                "valueAxis": [{
                    "show": true,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "axisScale": "linear",
                    "gridlineShow": true,
                    "gridlineColor": { "solid": { "color": "#E8E8E8" } },
                    "gridlineThickness": 1,
                    "fontFamily": "Roboto",
                    "fontSize": 10,
                    "labelDisplayUnits": 0
                }],
                "lineStyles": [{
                    "strokeWidth": 3,
                    "lineStyle": "solid",
                    "showMarker": true,
                    "markerSize": 5
                }]
            }
        },
        
        "barChart": {
            "*": {
                "legend": [{
                    "show": true,
                    "position": "Bottom",
                    "showTitle": false,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }],
                "categoryAxis": [{
                    "show": true,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }],
                "valueAxis": [{
                    "show": true,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "gridlineShow": true,
                    "gridlineColor": { "solid": { "color": "#E8E8E8" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10,
                    "labelDisplayUnits": 0
                }],
                "dataLabels": [{
                    "show": false,
                    "color": { "solid": { "color": "#2C3E50" } },
                    "fontSize": 9,
                    "fontFamily": "Roboto"
                }]
            }
        },
        
        "donutChart": {
            "*": {
                "legend": [{
                    "show": true,
                    "position": "Right",
                    "showTitle": false,
                    "labelColor": { "solid": { "color": "#34495E" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }],
                "labels": [{
                    "show": true,
                    "style": "Category, percent of total",
                    "color": { "solid": { "color": "#2C3E50" } },
                    "fontSize": 10,
                    "fontFamily": "Roboto",
                    "labelDisplayUnits": 0,
                    "labelPrecision": 0
                }],
                "slices": [{
                    "innerRadiusRatio": 50
                }]
            }
        },
        
        "table": {
            "*": {
                "grid": [{
                    "gridVertical": false,
                    "gridHorizontal": true,
                    "gridColor": { "solid": { "color": "#E8E8E8" } },
                    "gridThickness": 1,
                    "rowPadding": 3
                }],
                "columnHeaders": [{
                    "fontColor": { "solid": { "color": "#FFFFFF" } },
                    "backColor": { "solid": { "color": "#1E4D8B" } },
                    "fontFamily": "Roboto",
                    "fontSize": 11,
                    "fontWeight": "semibold"
                }],
                "values": [{
                    "fontColor": { "solid": { "color": "#2C3E50" } },
                    "backColor": { "solid": { "color": "#FFFFFF" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10,
                    "urlIcon": false,
                    "wordWrap": true
                }],
                "total": [{
                    "fontColor": { "solid": { "color": "#2C3E50" } },
                    "backColor": { "solid": { "color": "#ECF0F1" } },
                    "fontFamily": "Roboto",
                    "fontSize": 11,
                    "fontWeight": "bold"
                }]
            }
        },
        
        "slicer": {
            "*": {
                "data": [{
                    "mode": "Between",
                    "fontColor": { "solid": { "color": "#2C3E50" } },
                    "background": { "solid": { "color": "#FFFFFF" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }],
                "selection": [{
                    "selectAllCheckboxEnabled": true,
                    "singleSelect": false
                }],
                "header": [{
                    "show": true,
                    "fontColor": { "solid": { "color": "#2C3E50" } },
                    "background": { "solid": { "color": "#ECF0F1" } },
                    "fontFamily": "Roboto",
                    "fontSize": 11,
                    "fontWeight": "semibold"
                }],
                "items": [{
                    "fontColor": { "solid": { "color": "#34495E" } },
                    "background": { "solid": { "color": "#FFFFFF" } },
                    "fontFamily": "Roboto",
                    "fontSize": 10
                }]
            }
        },
        
        "kpi": {
            "*": {
                "indicator": [{
                    "indicatorDisplayUnits": 0,
                    "indicatorPrecision": 0,
                    "fontSize": 32,
                    "fontFamily": "Roboto",
                    "fontWeight": "bold"
                }],
                "trendline": [{
                    "show": true
                }],
                "goals": [{
                    "showGoal": true,
                    "showDistance": true
                }],
                "status": [{
                    "direction": "Positive",
                    "goodColor": { "solid": { "color": "#27AE60" } },
                    "neutralColor": { "solid": { "color": "#F39C12" } },
                    "badColor": { "solid": { "color": "#E74C3C" } }
                }]
            }
        }
    }
}